{
  "name": "nuxtapp",
  "version": "1.0.0",
  "description": "Application in Vue and Go.",
  "author": "Joseph Spurrier",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 8080",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest test/unit",
    "test:e2e": "jest test/e2e",
    "test:all": "jest",
    "test:watch": "jest --runInBand --watch",
    "test:debug": "node --inspect node_modules/.bin/jest",
    "test:cover": "jest test/unit --coverage",
    "coveralls": "jest test/unit --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/bulma": "^1.2.7",
    "@nuxtjs/pwa": "^2.6.0",
    "cookieparser": "^0.1.0",
    "js-cookie": "^2.2.1",
    "nuxt": "^2.11.0",
    "shortid": "^2.2.15",
    "vee-validate": "^2.2.15"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.9",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-nuxt": "^0.5.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-puppeteer": "^4.4.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "puppeteer": "^1.20.0",
    "typescript": "^2.9.2",
    "vue-jest": "^3.0.5"
  }
}
